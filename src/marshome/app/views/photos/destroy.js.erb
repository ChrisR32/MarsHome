$('#photos').html("<%= j render 'photos_list' %>")
<% if @listing.photos.blank? %>
    $('#photo_check').hide();
<% end %>

<% if !@listing.active && !@listing.title.blank? && !@listing.bed_room.blank? && !@listing.address_zone.blank? && !@listing.photos.blank? && !@listing.price.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>